#!/bin/sh

export PATH="${PATH}:${HOME}/.local/scripts"
export _JAVA_AWT_WM_NONREPARENTING=1
QT_QPA_PLATFORMTHEME=qt5ct

setxkbmap -layout hu
xbindkeys
xmodmap -e 'clear Lock' -e 'keycode 0x42 = Escape'
xset r rate 250 30
fehbg
picom &
dunst &
batsignal -e -b &
udiskie &
libinput-gestures-setup autostart start

